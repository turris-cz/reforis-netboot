#: js/src/app.js:11 js/src/netboot/Netboot.js:21
msgid "Netboot"
msgstr "நெட்பூட்"

#: js/src/netboot/Devices.js:48
msgid "Devices List"
msgstr "சாதனங்கள் பட்டியல்"

#: js/src/netboot/Devices.js:50
msgid "This list contains all devices which can be booted from this router."
msgstr ""
"இந்த பட்டியலில் இந்த திசைவியிலிருந்து துவக்கக்கூடிய அனைத்து சாதனங்களும் "
"உள்ளன."

#: js/src/netboot/DevicesTable.js:25
msgid "No netboot devices available."
msgstr "நெட்பூட் சாதனங்கள் எதுவும் கிடைக்கவில்லை."

#: js/src/netboot/DevicesTable.js:36
msgid "Serial Number"
msgstr "வரிசை எண்"

#: js/src/netboot/DevicesTable.js:39 js/src/netboot/DevicesTableRow.js:71
msgid "Paired"
msgstr "இணை"

#: js/src/netboot/DevicesTable.js:44
msgid "Change status"
msgstr "நிலையை மாற்றவும்"

#: js/src/netboot/DevicesTable.js:46
msgid "Action"
msgstr "செயல்"

#: js/src/netboot/DevicesTableRow.js:76
msgid "Awaiting acceptance"
msgstr "ஏற்றுக்கொள்வதற்காக காத்திருக்கிறது"

#: js/src/netboot/DevicesTableRow.js:81
msgid "Unknown status"
msgstr "தெரியாத நிலை"

#: js/src/netboot/DevicesTableRow.js:111
msgid "Unpair device"
msgstr "அவிழ்ப்பதற்கான சாதனம்"

#: js/src/netboot/DevicesTableRow.js:124
msgid "Accept pairing request"
msgstr "இணைத்தல் கோரிக்கையை ஏற்றுக்கொள்ளுங்கள்"

#: js/src/netboot/Netboot.js:23
msgid "Manage devices which can be booted from this router through network."
msgstr ""
"பிணையம் மூலம் இந்த திசைவியிலிருந்து துவக்கக்கூடிய சாதனங்களை "
"நிர்வகிக்கவும்."

#: js/src/netboot/hooks.js:74
msgid "Cannot pair devices."
msgstr "சாதனங்களை இணைக்க முடியாது."

#: reforis_netboot/__init__.py:42
msgid "Cannot accept pairing request."
msgstr "இணைத்தல் கோரிக்கையை ஏற்க முடியாது."

#: reforis_netboot/__init__.py:51
msgid "Cannot unpair devices."
msgstr "சாதனங்களைத் திறக்க முடியாது."

