msgstr "Spárované"

#: js/src/netboot/DevicesTable.js:44
msgid "Change status"
msgstr "Zmeniť stav"

#: js/src/netboot/DevicesTable.js:46
msgid "Action"
msgstr "Akcia"

#: js/src/netboot/DevicesTableRow.js:67
msgid "Unknown status"
msgstr "Neznámy stav"

#: js/src/netboot/DevicesTableRow.js:74
msgid "Awaiting acceptance"
msgstr "Čaká sa na prijatie"

